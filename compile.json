{
  "$schema": "https://qooxdoo.org/schema/compile-1-0-0.json",
  "environment": {
    "cv.version": "dev",
    "cv.xhr": "qx",
    "cv.testMode": false
  },
  "locales": [
    "en",
    "de"
  ],
  "applications": [
    {
      "class": "cv.Application",
      "name": "cv",
      "theme": "cv.theme.Dark",
      "type": "browser",
      "environment": {
        "qx.promise": false,
        "qx.command.bindEnabled": true
      },
      "default": true,
      "include": [
        "cv.Application",
        "cv.ui.structure.pure.*",
        "cv.ui.structure.tile.*",
        "cv.ui.manager.*",
        "cv.transforms.*",
        "cv.plugins.*",
        "cv.parser.*",
        "cv.core.*",
        "cv.io.listmodel.*"
      ],
      "exclude": [
        "cv.test.*",
        "qx.test.*"
      ],
      "bootPath": "source/boot",
      "parts": {
        "boot": {
          "include": [
            "cv.Application",
            "cv.transforms.*",
            "cv.parser.widgets.*",
            "cv.core.notifications.actions.*",
            "cv.ui.structure.pure.AbstractBasicWidget",
            "cv.ui.structure.pure.AbstractWidget",
            "cv.ui.structure.pure.ColorChooser",
            "cv.io.listmodel.*"
          ]
        },
        "structure-pure": {
          "include": [
            "cv.ui.structure.pure.*"
          ],
          "exclude": [
            "cv.ui.structure.pure.AbstractBasicWidget",
            "cv.ui.structure.pure.AbstractWidget",
            "cv.ui.structure.pure.ColorChooser"
          ]
        },
        "structure-tile": {
          "include": [
            "cv.ui.structure.tile.*"
          ]
        },
        "manager": {
          "include": [
            "cv.ui.manager.*"
          ]
        },
        "plugin-diagram": {
          "include": [
            "cv.plugins.diagram.*"
          ]
        },
        "plugin-clock": {
          "include": [
            "cv.plugins.Clock"
          ]
        },
        "plugin-gauge": {
          "include": [
            "cv.plugins.Gauge"
          ]
        },
        "plugin-link": {
          "include": [
            "cv.plugins.Link"
          ]
        },
        "plugin-mobilemenu": {
          "include": [
            "cv.plugins.MobileMenu"
          ]
        },
        "plugin-powerspectrum": {
          "include": [
            "cv.plugins.PowerSpectrum"
          ]
        },
        "plugin-openweathermap": {
          "include": [
            "cv.plugins.OpenweatherMap"
          ]
        },
        "plugin-rss": {
          "include": [
            "cv.plugins.Rss"
          ]
        },
        "plugin-rsslog": {
          "include": [
            "cv.plugins.RssLog"
          ]
        },
        "plugin-strftime": {
          "include": [
            "cv.plugins.Strftime"
          ]
        },
        "plugin-speech": {
          "include": [
            "cv.plugins.Speech"
          ]
        },
        "plugin-timeout": {
          "include": [
            "cv.plugins.Timeout"
          ]
        },
        "plugin-tr064": {
          "include": [
            "cv.plugins.tr064.*"
          ]
        },
        "plugin-svg": {
          "include": [
            "cv.plugins.Svg"
          ]
        },
        "plugin-upnpcontroller": {
          "include": [
            "cv.plugins.UpnpController"
          ]
        },
        "plugin-openhab": {
          "include": [
            "cv.plugins.openhab.*"
          ]
        }
      }
    },
    {
      "class": "qxl.apiviewer.Application",
      "theme": "qxl.apiviewer.Theme",
      "name": "apiviewer",
      "title": "CometVisu API Viewer",
      "environment": {
        "qx.icontheme": "Tango",
        "qxl.apiviewer.applicationName": "cv",
        "qxl.apiviewer.exclude": ["qxl.apiviewer.*", "q.*", "qx.test.*", "q", "qxWeb"],
        "qxl.versionlabel.name": "CometVisu",
        "qxl.versionlabel.version": "dev"
      },
      "include": [
        "qx.*"
      ],
      "exclude": ["qx.module.Blocker","qx.module.Placement"],
      "bootPath": "../../source/apiboot",
      "parts": {
        "boot": {
          "include": ["qx.*"]
        },
        "tool": {
          "include": ["qx.tool.*"]
        }
      }
    }
  ],
  "libraries": [
    "client",
    "."
  ],
  "targets": [
    {
      "type": "source",
      "outputPath": "compiled/source",
      "application-types": [
        "browser"
      ],
      "babelOptions": {
        "targets": "edge >=18, chrome >= 73, firefox >= 66"
      },
      "environment": {
        "cv.build": "source",
        "cv.sentry": false,
        "qx.globalErrorHandling": false
      },
      "bundle": {
        "include": ["qx.*"]
      }
    },
    {
      "type": "build",
      "application-types": [
        "browser"
      ],
      "outputPath": "compiled/build",
      "environment": {
        "cv.build": "build",
        "cv.sentry": true,
        "qx.globalErrorHandling": true,
        "qx.debug": false
      }
    }
  ],
  "defaultTarget": "source",
  "serve": {
    "listenPort": 8082
  },
  "sass": {
    "compiler": "latest"
  },
  "ignores": [
    "require",
    "process",
    "loadSass",
    "__dirname",
    "$",
    "EVENT_RECORDER",
    "EventSource",
    "Favico",
    "jQuery",
    "localStorage",
    "monaco",
    "Paho",
    "replayLog",
    "Sentry",
    "sprintf",
    "svg4everybody",
    "THREE",
    "Worker",
    "Node",
    "indexedDB",
    "URL",
    "sinon",
    "XMLDocument",
    "HTMLElement",
    "customElements",
    "QxConnector",
    "RSSParser",
    "Response",
    "observedAttributes"
  ]
}
