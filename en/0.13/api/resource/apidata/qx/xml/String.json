      "jsdoc": {
        "@description": [
          {
            "name": "@description",
            "body": "<p>Escapes the characters in a <code>String</code> using XML entities.\n For example: <tt>\"bread\" & \"butter\"</tt> =&gt;\n <tt>&amp;quot;bread&amp;quot; &amp;amp; &amp;quot;butter&amp;quot;</tt>.\n Supports only the four basic XML entities (gt, lt, quot, amp).\n Does not support DTDs or external entities.\n Note that unicode characters greater than 0x7f are currently escaped to their numerical \\u equivalent.</p>"
          }
        ],
        "@param": [
          {
            "name": "@param",
            "body": "str {String} the string to be escaped",
            "paramName": "str",
            "description": " the string to be escaped",
            "type": "qx.xml.String"
          }
        ],
        "@return": [
          {
            "name": "@return",
            "body": "{String} the escaped string",
            "type": "qx.xml.String",
            "description": " the escaped string"
          }
        ]
