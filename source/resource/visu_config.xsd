      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="shortupvalue" type="xsd:string" use="optional">
      <xsd:annotation>
      	  <xsd:documentation xml:lang="en">Defines the value which is sent on a short press of the left button.</xsd:documentation>
      	  <xsd:documentation xml:lang="de">Legt fest welcher Wert bei kurzer Betätigung der linken Schaltfläche gesendet wird.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="downvalue" type="xsd:string" use="optional">
      <xsd:annotation>
      	  <xsd:documentation xml:lang="en">Defines the value which is sent on a long press of the right button.</xsd:documentation>
      	  <xsd:documentation xml:lang="de">Legt fest welcher Wert bei langer Betätigung der rechten Schaltfläche gesendet wird.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="shortdownvalue" type="xsd:string" use="optional">
      <xsd:annotation>
      	  <xsd:documentation xml:lang="en">Defines the value which is sent on a short press of the right button.</xsd:documentation>
      	  <xsd:documentation xml:lang="de">Legt fest welcher Wert bei kurzer Betätigung der rechten Schaltfläche gesendet wird.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="uplabel" type="xsd:string" use="optional">
      <xsd:annotation>
      	  <xsd:documentation xml:lang="en">Defines the label which is displayed on the left button.</xsd:documentation>
      	  <xsd:documentation xml:lang="de">Legt die Beschriftung der linken Schaltfläche fest.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="downlabel" type="xsd:string" use="optional">
      <xsd:annotation>
      	  <xsd:documentation xml:lang="en">Defines the label which is displayed on the right button.</xsd:documentation>
      	  <xsd:documentation xml:lang="de">Legt die Beschriftung der rechten Schaltfläche fest.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="shorttime" type="xsd:decimal" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Time in milliseconds that is accepted as shorttime.</xsd:documentation>
        <xsd:documentation xml:lang="de">Zeit in Millisekunden innerhalb der die Betätigung der Schaltfläche als Kurzzeit interpretiert wird.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="change" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Might be 'absolute' or 'relative'.</xsd:documentation>
      </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="absolute" />
            <xsd:enumeration value="relative" />
          </xsd:restriction>
        </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute ref="min" />
    <xsd:attribute ref="max" />
    <xsd:attribute ref="mapping" use="optional" />
    <xsd:attribute ref="styling" use="optional" />
    <xsd:attribute ref="align" use="optional" />
    <xsd:attribute ref="format" use="optional" />
    <xsd:attribute name="infoposition" use="optional">
        <xsd:annotation>
            <xsd:appinfo>level:expert</xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="left" />
            <xsd:enumeration value="middle" />
            <xsd:enumeration value="right" />
          </xsd:restriction>
        </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute ref="flavour" use="optional" />
    <xsd:attribute ref="class" use="optional" />
  </xsd:complexType>
  
