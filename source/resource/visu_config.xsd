  <xsd:complexType name="diagram">
    <xsd:sequence>
      <xsd:element name="layout" type="layout" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="label" type="label" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="axis" type="axis" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="rrd" type="rrd" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:attribute name="width" type="dimension" use="optional" />
    <xsd:attribute name="height" type="dimension" use="optional" />
    <xsd:attribute name="popup" type="xsd:boolean" use="optional" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Makes diagram clickable and opens fullscreen view of diagram when clicked</xsd:documentation>
        <xsd:documentation xml:lang="de">Aktiviert die Popup-Funktion, die ein Vollbilddiagramm anzeigt, wenn das Diagramm angeklickt wird</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="previewlabels" type="xsd:boolean" use="optional" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Show labels on inline-diagram in widget</xsd:documentation>
        <xsd:documentation xml:lang="de">Zeigt die Achsenbeschriftung im Inline-diagramm an</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>  
    <xsd:attribute ref="series" use="optional" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Defines the base unit for the period attribute</xsd:documentation>
        <xsd:documentation xml:lang="de">Legt die Einheit des period Attributs fest</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="refresh" type="xsd:integer" use="optional" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">refresh-rate in seconds, no refresh if missing</xsd:documentation>
        <xsd:documentation xml:lang="de">Aktualisierungsintervall des Diagramms ins Sekundens</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="period" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Defines whether an interval of hours, weeks, month or years is displayed (as multiple of "series)"</xsd:documentation>
        <xsd:documentation xml:lang="de">Definiert ob ein Stunden-, Tage-, Wochen- oder Jahresbereich dargestellt werden soll (als Vielfaches von "series")</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="gridcolor" type="xsd:string" use="optional" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Color of the grid shown behind the graphs</xsd:documentation>
        <xsd:documentation xml:lang="de">Farbe der Gitternetzlinien im Diagramm</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute ref="legend" use="optional" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Defines whether legend is shown in combination with popup attribute. "none" => no legend, "both" => legend is shown in small inline diagram and popup, "inline" => legend in inline diagram only, "popup" => legend is only dispayed in popup diagram</xsd:documentation>
        <xsd:documentation xml:lang="de">Definiert ob eine Legende angezeigt werden soll (in Verbindung mit popup Parameter). "None" => keine Legende, "both" => Legende im kleinen Inline Diagramm und Popup, "inline" => Legende nur im Inline Diagramm, "popup" => Legende nur im Popup Diagramm</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute ref="legendposition" use="optional" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Defines position of legend in diagram</xsd:documentation>
        <xsd:documentation xml:lang="de">Legt die Position der Legende im Diagramm fest</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="title" type="xsd:string" use="optional" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Adds a title to the diagram</xsd:documentation>
        <xsd:documentation xml:lang="de">Fügt dem Diagramm einen Titel hinzu</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="tooltip" type="xsd:boolean" use="optional" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Enables a tooltip which displays the current value where the graph is clicked</xsd:documentation>
        <xsd:documentation xml:lang="de">Aktiviert die Tooltip-Funktion, die den aktuellen Wert anzeigt, an der die Diagrammlinie angeklickt wird</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute ref="timeformat" use="optional" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Defines time format for x-axis</xsd:documentation>
        <xsd:documentation xml:lang="de">Legt das Datums-Zeitformat für x-Achse fest</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute ref="timeformatTooltip" use="optional" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Defines time format for displayed values if tooltip is used</xsd:documentation>
        <xsd:documentation xml:lang="de">Legt das Datums-Zeitformat für die im Tooltip angezeigten Werte fest</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="zoomYAxis" type="xsd:boolean" use="optional" >
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Defines whether zoom acts on y-axis in addition to x-axis</xsd:documentation>
        <xsd:documentation xml:lang="de">Legt fest, ob beim Zoomen die y-Achse zusätzlich zur x-Achse skaliert wird</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute ref="seriesStart" use="optional" />
    <xsd:attribute ref="seriesEnd" use="optional" />
    <xsd:attribute ref="seriesResolution" use="optional" />
  </xsd:complexType>
  
  <xsd:complexType name="diagram_info">
    <xsd:sequence>
      <xsd:element name="layout" type="layout" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="label" type="label" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="axis" type="axis" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="rrd" type="rrd" minOccurs="0" maxOccurs="unbounded"/>
      <xsd:element name="address" type="address" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
    <xsd:attribute ref="format" use="optional" />
    <xsd:attribute ref="styling" use="optional" />
    <xsd:attribute ref="mapping" use="optional" />
    <xsd:attribute ref="align" use="optional" />
    <xsd:attribute ref="bind_click_to_widget" use="optional"/>
    <xsd:attribute ref="series" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Defines the base unit for the period attribute eg. day, hour</xsd:documentation>
        <xsd:documentation xml:lang="de">Legt die Einheit des period Attributs fest zB. day, hour</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="refresh" type="xsd:integer" use="optional" />
    <xsd:attribute name="period" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Time period shown in the diagram as multiple of "series"</xsd:documentation>
        <xsd:documentation xml:lang="de">Zeitraum als Vielfaches von "series" der im Diagram dargestellt wird</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="gridcolor" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation xml:lang="en">Color of the grid shown behind the graphs</xsd:documentation>
        <xsd:documentation xml:lang="de">Farbe der Gitternetzlinien im Diagramm</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute ref="legend" use="optional" />
    <xsd:attribute ref="legendposition" use="optional" />
    <xsd:attribute name="title" type="xsd:string" use="optional" />
    <xsd:attribute name="tooltip" type="xsd:boolean" use="optional" />
    <xsd:attribute ref="timeformat" use="optional" />
    <xsd:attribute ref="timeformatTooltip" use="optional" />
    <xsd:attribute name="zoomYAxis" type="xsd:boolean" use="optional" />
    <xsd:attribute ref="seriesStart" use="optional" />
    <xsd:attribute ref="seriesEnd" use="optional" />
    <xsd:attribute ref="seriesResolution" use="optional" />
  </xsd:complexType>

