<?xml version="1.0" encoding="UTF-8"?>
<pages design="discreet" lib_version="6" screensave_time="500000" screensave_page="id_" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../visu_config.xsd">
  <meta>
    <plugins>
      <plugin name="colorchooser"/>
      <plugin name="clock"/>
      <plugin name="diagram"/>
      <plugin name="gauge"/>
    </plugins>
    <icons/>
    <mappings>
      <mapping name="Weather">
        <entry value="0">0:<icon name="weather_test" styling="height:8em;"/></entry>
        <entry value="1">Off</entry>
      </mapping>
      <mapping name="OnOff">
        <entry value="0">On</entry>
        <entry value="1">Off</entry>
      </mapping>
      <mapping name="OffOn">
        <entry value="0">Off</entry>
        <entry value="1">On</entry>
      </mapping>
      <mapping name="OpenClose">
        <entry value="0">Open</entry>
        <entry value="1">Close</entry>
      </mapping>
      <mapping name="CloseOpen">
        <entry value="0">Close</entry>
        <entry value="1">Open</entry>
      </mapping>
      <mapping name="Sign">
        <entry range_min="-1e99" range_max="0">Negative</entry>
        <entry value="0">Zero</entry>
        <entry range_min="0" range_max="1e99">Positive</entry>
      </mapping>
      <mapping name="LichtEG">
        <entry value="0">
          <icon name="control_building_modern_s_eg" color="white" type="64" styling="height:auto"/>
        </entry>
        <entry value="1">
          <icon name="control_building_modern_s_eg" color="red" type="64" styling="height:auto"/>
        </entry>
      </mapping>
      <mapping name="OnOff_Licht">
        <entry value="0">
          <icon name="light_light" color="white"/>
        </entry>
        <entry value="1">
          <icon name="light_light_dim_100" color="orange"/>
        </entry>
      </mapping>
      <mapping name="DaWegIcon">
        <entry value="0">
          <icon name="user_awayX" color="white"/>
        </entry>
        <entry value="1">
          <icon name="user_available" color="#64e92c"/>
        </entry>
      </mapping>
      <mapping name="Jetzt">
        <entry value="0">
          <icon name="control_on_off" color="orange"/>
        </entry>
      </mapping>
    </mappings>
    <stylings>
      <styling name="RedGreen">
        <entry value="0">red</entry>
        <entry value="1">green</entry>
      </styling>
      <styling name="GreenRed">
        <entry value="0">green</entry>
        <entry value="1">red</entry>
      </styling>
      <styling name="BluePurpleRed_Temp">
        <entry range_min="-100" range_max="0">blue</entry>
        <entry value="0">purple</entry>
        <entry range_min="0" range_max="100">red</entry>
      </styling>
      <styling name="BlueRed_Humi">
        <entry range_min="0" range_max="60">blue</entry>
        <entry range_min="61" range_max="100">red</entry>
      </styling>
      <styling name="GreyGrey">
        <entry value="0">grey</entry>
        <entry value="1">grey</entry>
      </styling>
    </stylings>
    <statusbar>
      <status type="html">&lt;img src="icon/comet_64_ff8000.png" alt="CometVisu" /&gt; by &lt;a href="http://www.cometvisu.org/"&gt;CometVisu.org&lt;/a&gt;
                                - &lt;a href="."&gt;Reload&lt;/a&gt;
                                - &lt;a href="?config=demo"&gt;Widget Demo&lt;/a&gt;</status>
      <status type="html" condition="!edit" hrefextend="config">- &lt;a href="editor/"&gt;Edit&lt;/a&gt;</status>
      <status type="html" condition="edit" hrefextend="all">- &lt;a href="."&gt;normal Mode&lt;/a&gt;</status>
    </statusbar>
  </meta>
