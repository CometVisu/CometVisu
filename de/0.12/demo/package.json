    "grunt-banner": "^0.6.0",
    "grunt-bump": "^0.8.0",
    "grunt-chmod": "latest",
    "grunt-composer": "^0.4.5",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-compress": "^1.5.0",
    "grunt-contrib-connect": "^2.1.0",
    "grunt-contrib-uglify": "^4.0.1",
    "grunt-file-creator": "^0.1.3",
    "grunt-github-changes": "latest",
